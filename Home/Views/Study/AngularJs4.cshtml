@{
    Layout = null;
}
<!DOCTYPE html>
<html ng-app="sportsStoreAdmin">
<head>
    <title>SportsStore</title>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <!-- 합쳐지고 최소화된 최신 CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css">
    <!-- 부가적인 테마 -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap-theme.min.css">
    <!-- 합쳐지고 최소화된 최신 자바스크립트 -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/js/bootstrap.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.10/angular.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.10/angular-route.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.10/angular-resource.js"></script>
    <script>
        angular.module("sportsStoreAdmin", ["ngRoute", "ngResource"])
            .config(function ($routeProvider) {
                $routeProvider.when("/login", {
                    templateUrl: "/Content/angularStudy/adminLogin.html"
                });
                $routeProvider.when("/main", {
                    templateUrl: "/Content/angularStudy/adminMain.html"
                });
                $routeProvider.otherwise({
                    templateUrl: "/Content/angularStudy/adminMain.html"
                });
            });
    </script>
    <script src="/Content/angularStudy/adminControllers.js"></script>
    <script src="/Content/angularStudy/adminProductController.js"></script>
</head>
<body>
    <ng-view />
</body>
</html>